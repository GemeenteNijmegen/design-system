{
  "nijmegen": {
    "badge": {
      "font-family": {
        "$type": "fontFamilies",
        "$value": "{utrecht.document.font-family}"
      },
      "font-weight": {
        "$type": "fontWeights",
        "$value": "{nijmegen.typography.font-weight.semibold}"
      },
      "line-height": {
        "$type": "lineHeights",
        "$value": "{utrecht.document.line-height}"
      },
      "font-size": {
        "$type": "fontSizes",
        "$value": "{utrecht.document.font-size}"
      },
      "positive": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.feedback.success.default}"
        },
        "background-color": {
          "$type": "color",
          "$value": "{nijmegen.color.feedback.success.subdued}"
        },
        "border-color": {
          "$type": "color",
          "$value": "{nijmegen.color.none}"
        }
      },
      "border-width": {
        "$type": "borderWidth",
        "$value": "{nijmegen.border-width.none}"
      },
      "border-radius": {
        "$type": "borderRadius",
        "$value": "{nijmegen.border-radius.sm}"
      },
      "padding-block-end": {
        "$type": "spacing",
        "$value": "{nijmegen.space.25}"
      },
      "padding-block-start": {
        "$type": "spacing",
        "$value": "{nijmegen.space.25}"
      },
      "padding-inline-end": {
        "$type": "spacing",
        "$value": "{nijmegen.space.50}"
      },
      "padding-inline-start": {
        "$type": "spacing",
        "$value": "{nijmegen.space.50}"
      },
      "negative": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.feedback.error.default}"
        },
        "background-color": {
          "$type": "color",
          "$value": "{nijmegen.color.feedback.error.subdued}"
        },
        "border-color": {
          "$type": "color",
          "$value": "{nijmegen.color.none}"
        }
      },
      "warning": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.feedback.warning.default}"
        },
        "background-color": {
          "$type": "color",
          "$value": "{nijmegen.color.feedback.warning.subdued}"
        },
        "border-color": {
          "$type": "color",
          "$value": "{nijmegen.color.none}"
        }
      },
      "informative": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.feedback.info.default}"
        },
        "background-color": {
          "$type": "color",
          "$value": "{nijmegen.color.feedback.info.subdued}"
        },
        "border-color": {
          "$type": "color",
          "$value": "{nijmegen.color.none}"
        }
      }
    }
  }
}