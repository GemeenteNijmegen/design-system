{
  "utrecht": {
    "link-list": {
      "row-gap": {
        "$type": "spacing",
        "$value": "{nijmegen.space.50}"
      },
      "icon": {
        "inset-block-start": {
          "$type": "spacing",
          "$value": "{nijmegen.space.12}"
        },
        "size": {
          "$type": "sizing",
          "$value": "{nijmegen.icon.functional.size}"
        }
      }
    }
  },
  "todo": {
    "link-list": {
      "item": {
        "column-gap": {
          "$type": "spacing",
          "$value": "{nijmegen.space.50}"
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "None"
        },
        "active": {
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "underline"
          }
        },
        "focus": {
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "None"
          }
        },
        "hover": {
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "underline"
          }
        }
      }
    }
  }
}