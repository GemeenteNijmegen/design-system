{
  "utrecht": {
    "link": {
      "color": {
        "$type": "color",
        "$value": "{nijmegen.interaction.color}"
      },
      "text-decoration-color": {
        "$type": "color",
        "$value": "{nijmegen.interaction.color}"
      },
      "active": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.interaction.active.color}"
        }
      },
      "focus": {
        "background-color": {
          "$type": "color",
          "$value": "{nijmegen.focus.background-color}"
        },
        "color": {
          "$type": "color",
          "$value": "{nijmegen.focus.color}"
        }
      },
      "focus-visible": {
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "None"
        },
        "text-decoration-thickness": {
          "$type": "other",
          "$value": "auto"
        }
      },
      "hover": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.interaction.hover.color}"
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "None"
        },
        "text-decoration-thickness": {
          "$type": "other",
          "$value": "auto"
        }
      },
      "visited": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.interaction.color}"
        }
      },
      "text-decoration": {
        "$type": "textDecoration",
        "$value": "underline"
      },
      "icon": {
        "size": {
          "$type": "sizing",
          "$value": "{nijmegen.icon.functional.size}"
        }
      },
      "text-decoration-thickness": {
        "$type": "other",
        "$value": "auto"
      },
      "text-underline-offset": {
        "$type": "other",
        "$value": "auto"
      },
      "column-gap": {
        "$type": "spacing",
        "$value": "{nijmegen.space.25}"
      }
    }
  }
}