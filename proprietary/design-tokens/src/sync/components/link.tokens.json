{
  "utrecht": {
    "link": {
      "line-height": {
        "$type": "lineHeights",
        "$value": "{utrecht.document.line-height}"
      },
      "color": {
        "$type": "color",
        "$value": "{nijmegen.interaction.color}"
      },
      "text-decoration-color": {
        "$type": "color",
        "$value": "{nijmegen.interaction.color}"
      },
      "active": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.interaction.active.color}"
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "none"
        }
      },
      "focus": {
        "background-color": {
          "$type": "color",
          "$value": "{nijmegen.focus.background-color}"
        },
        "color": {
          "$type": "color",
          "$value": "{nijmegen.focus.color}"
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "none"
        },
        "text-decoration-thickness": {
          "$type": "other",
          "$value": "auto"
        }
      },
      "hover": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.interaction.hover.color}"
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "none"
        },
        "text-decoration-thickness": {
          "$type": "other",
          "$value": "auto"
        }
      },
      "visited": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.interaction.color}"
        }
