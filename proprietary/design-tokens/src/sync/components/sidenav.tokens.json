{
  "denhaag": {
    "sidenav": {
      "link": {
        "line-height": {
          "$type": "lineHeights",
          "$value": "{utrecht.document.line-height}"
        },
        "icon": {
          "size": {
            "$type": "sizing",
            "$value": "{nijmegen.icon.functional.size}"
          },
          "margin-inline": {
            "$type": "spacing",
            "$value": "{nijmegen.space.inline.xs}"
          }
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{utrecht.document.font-size}"
        },
        "font-family": {
          "$type": "fontFamilies",
          "$value": "{utrecht.document.font-family}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{utrecht.document.font-weight}"
        },
        "current": {
          "font-weight": {
            "$type": "fontWeights",
            "$value": "{nijmegen.document.strong.font-weight}"
          },
          "color": {
            "$type": "color",
            "$value": "{utrecht.document.color}"
          }
        },
        "color": {
          "$type": "color",
          "$value": "{nijmegen.interaction.color}"
        },
        "active": {
          "color": {
            "$type": "color",
            "$value": "{nijmegen.interaction.active.color}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "underline"
          }
        },
        "focus": {
          "background-color": {
            "$type": "color",
            "$value": "{nijmegen.focus.background-color}"
          },
          "color": {
            "$type": "color",
            "$value": "{nijmegen.focus.color}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "none"
          }
        },
        "hover": {
          "color": {
            "$type": "color",
            "$value": "{nijmegen.interaction.hover.color}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "underline"
          }
        },
        "padding-block-start": {
          "$type": "spacing",
          "$value": "{nijmegen.space.block.sm}"
        },
        "padding-block-end": {
          "$type": "spacing",
          "$value": "{nijmegen.space.block.sm}"
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "none"
        }
      }
    }
  }
}