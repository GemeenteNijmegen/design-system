{
  "todo": {
    "accordion": {
      "border-radius": {
        "$type": "borderRadius",
        "$value": "{nijmegen.border-radius.none}"
      },
      "section": {
        "border-block-end-width": {
          "$type": "borderWidth",
          "$value": "{nijmegen.border-width.sm}"
        },
        "border-color": {
          "$type": "color",
          "$value": "{nijmegen.color.border.subdued}"
        }
      },
      "button": {
        "font-size": {
          "$type": "fontSizes",
          "$value": "{utrecht.document.font-size}"
        },
        "line-height": {
          "$type": "lineHeights",
          "$value": "{nijmegen.typography.line-height.sm}"
        },
        "expanded": {
          "background-color": {
            "$type": "color",
            "$value": "{nijmegen.color.grijs.100}"
          },
          "border-color": {
            "$type": "color",
            "$value": "transparent"
          },
          "color": {
            "$type": "color",
            "$value": "{nijmegen.interaction.color}"
          }
        },
        "font-family": {
          "$type": "fontFamilies",
          "$value": "{utrecht.document.font-family}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{nijmegen.typography.font-weight.semibold}"
        }
      }
    }
  },
  "utrecht": {
    "accordion": {
