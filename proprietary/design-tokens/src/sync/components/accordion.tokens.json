        "border-width": {
          "$type": "borderWidth",
          "$value": "{nijmegen.border-width.sm}"
        },
        "padding-block-end": {
          "$type": "spacing",
          "$value": "{nijmegen.space.150}"
        },
        "padding-block-start": {
          "$type": "spacing",
          "$value": "{nijmegen.space.150}"
        },
        "padding-inline-end": {
          "$type": "spacing",
          "$value": "{nijmegen.space.100}"
        },
        "padding-inline-start": {
          "$type": "spacing",
          "$value": "{nijmegen.space.100}"
        }
      },
      "button": {
        "gap": {
          "$type": "spacing",
          "$value": "{nijmegen.space.50}"
        },
        "icon": {
          "size": {
            "$type": "sizing",
            "$value": "{nijmegen.icon.functional.size}"
          }
        },
        "padding-block-end": {
          "$type": "spacing",
          "$value": "{nijmegen.space.75}"
        },
        "padding-block-start": {
          "$type": "spacing",
          "$value": "{nijmegen.space.75}"
        },
        "padding-inline-end": {
          "$type": "spacing",
          "$value": "{nijmegen.space.100}"
        },
        "padding-inline-start": {
          "$type": "spacing",
          "$value": "{nijmegen.space.100}"
        },
        "hover": {
          "background-color": {
            "$type": "color",
            "$value": "{nijmegen.color.background.hover}"
          },
          "border-color": {
            "$type": "color",
            "$value": "{nijmegen.color.none}"
          },
          "color": {
            "$type": "color",
            "$value": "{nijmegen.interaction.hover.color}"
          }
        },
        "background-color": {
          "$type": "color",
          "$value": "{nijmegen.color.background.subdued}"
        },
        "border-color": {
          "$type": "color",
          "$value": "{nijmegen.color.none}"
        },
        "color": {
          "$type": "color",
          "$value": "{nijmegen.interaction.color}"
        },
        "focus": {
          "background-color": {
            "$type": "color",
            "$value": "{nijmegen.focus.background-color}"
          },
          "border-color": {
            "$type": "color",
            "$value": "{nijmegen.color.none}"
          },
          "color": {
            "$type": "color",
            "$value": "{nijmegen.focus.color}"
          }
        },
        "active": {
          "background-color": {
            "$type": "color",
            "$value": "{nijmegen.color.grijs.300}"
          },
          "border-color": {
            "$type": "color",
            "$value": "{nijmegen.color.none}"
          },
          "color": {
            "$type": "color",
            "$value": "{nijmegen.interaction.active.color}"
          }
        },
        "border-width": {
          "$type": "borderWidth",
          "$value": "{nijmegen.border-width.sm}"
        },
        "border-radius": {
          "$type": "borderRadius",
          "$value": "{nijmegen.border-radius.none}"
        }
      }
    }
