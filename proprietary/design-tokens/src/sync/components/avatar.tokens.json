{
  "nl": {
    "avatar": {
      "size": {
        "$type": "sizing",
        "$value": "{utrecht.pointer-target.min-size}"
      },
      "icon": {
        "icon-size": {
          "$type": "sizing",
          "$value": "{nijmegen.icon.functional.size}"
        }
      },
      "background-color": {
        "$type": "color",
        "$value": "{nijmegen.interaction.color}"
      },
      "border-color": {
        "$type": "color",
        "$value": "transparent"
      },
      "color": {
        "$type": "color",
        "$value": "{nijmegen.color.wit}"
      },
      "text": {
        "font-family": {
          "$type": "fontFamilies",
          "$value": "{utrecht.document.font-family}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{nijmegen.typography.font-weight.semibold}"
        },
        "line-height": {
          "$type": "lineHeights",
          "$value": "{utrecht.document.line-height}"
        },
        "text-transform": {
          "$type": "textCase",
          "$value": "uppercase"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{utrecht.document.font-size}"
        }
      },
      "border-width": {
        "$type": "borderWidth",
        "$value": "{nijmegen.border-width.sm}"
      },
      "hover": {
        "background-color": {
          "$type": "color",
          "$value": "{nijmegen.interaction.hover.color}"
        },
        "border-color": {
          "$type": "color",
          "$value": "transparent"
        },
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.wit}"
        }
      }
    }
  }
}