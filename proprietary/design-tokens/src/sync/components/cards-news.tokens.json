{
  "nijmegen": {
    "card-nieuws": {
      "background-color": {
        "$type": "color",
        "$value": "{nijmegen.color.wit}"
      },
      "border-color": {
        "$type": "color",
        "$value": "{nijmegen.color.border.default}"
      },
      "border-radius": {
        "$type": "borderRadius",
        "$value": "{nijmegen.border-radius.xs}"
      },
      "content": {
        "padding": {
          "block-end": {
            "$type": "spacing",
            "$value": "{nijmegen.space.block.2xl}"
          },
          "block-start": {
            "$type": "spacing",
            "$value": "{nijmegen.space.block.xl}"
          },
          "inline": {
            "$type": "spacing",
            "$value": "{nijmegen.space.inline.xl}"
          }
        },
        "row": {
          "block-gap": {
            "$type": "spacing",
            "$value": "{nijmegen.space.row.md}"
          }
        }
      },
      "datum": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.grijs.400}"
        }
      },
      "heading": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.heading.color}"
        },
        "font-family": {
          "$type": "color",
          "$value": "{nijmegen.heading.font-family}"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{utrecht.heading-3.font-size}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{nijmegen.heading.font-weight}"
        },
        "hover": {
          "color": {
            "$type": "color",
            "$value": "{nijmegen.interaction.hover.color}"
          }
        },
        "line-height": {
          "$type": "lineHeights",
          "$value": "{utrecht.heading-3.line-height}"
        }
      },
      "paragraph": {
        "color": {
          "$type": "color",
          "$value": "{utrecht.paragraph.color}"
        }
      }
    },
    "card-news": {
      "font-family": {
        "$type": "fontFamilies",
        "$value": "{nijmegen.typography.font-family.secondary}"
      }
    }
  }
}