{
  "nijmegen": {
    "card-searchresults": {
      "base": {
        "background-color": {
          "$type": "color",
          "$value": "{nijmegen.color.wit}"
        },
        "border-width": {
          "$type": "borderWidth",
          "$value": "{nijmegen.line.border-width}"
        },
        "padding": {
          "inline": {
            "$type": "spacing",
            "$value": "{nijmegen.card-event.content.padding.inline}"
          },
          "block-start": {
            "$type": "spacing",
            "$value": "{nijmegen.space.block.xl}"
          },
          "block-end": {
            "$type": "spacing",
            "$value": "{nijmegen.space.block.xl}"
          }
        },
        "title": {
          "font-family": {
            "$type": "fontFamilies",
            "$value": "{nijmegen.heading.font-family}"
          },
          "font-size": {
            "$type": "fontSizes",
            "$value": "{1.5rem}"
          },
          "font-weight": {
            "$type": "fontWeights",
            "$value": "{nijmegen.typography.font-weight.semibold}"
          },
          "line-height": {
            "$type": "lineHeights",
            "$value": "{nijmegen.typography.line-height.md}"
          },
          "color": {
            "$type": "color",
            "$value": "{nijmegen.color.zwart}"
          }
        },
        "body": {
          "font-family": {
            "$type": "fontFamilies",
            "$value": "{nijmegen.typography.font-family.secondary}"
          },
          "font-size": {
            "$type": "fontSizes",
            "$value": "{nijmegen.typography.font-size.md}"
          },
          "font-weight": {
            "$type": "fontWeights",
            "$value": "{nijmegen.typography.font-weight.regular}"
          },
          "line-height": {
            "$type": "lineHeights",
            "$value": "{nijmegen.typography.line-height.sm}"
          },
          "color": {
            "$type": "color",
            "$value": "{nijmegen.color.grijs.500}"
          }
        },
        "link": {
          "color": {
            "$type": "color",
            "$value": "{nijmegen.color.groen.500}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "{nijmegen.hover.text-decoration}"
          }
        },
        "icon": {
          "color": {
            "$type": "color",
            "$value": "{nijmegen.color.grijs.400}"
          }
        },
        "metadata": {
          "color": {
            "$type": "color",
            "$value": "{nijmegen.color.grijs.400}"
          },
          "padding": {
            "inline": {
              "$type": "spacing",
              "$value": "{nijmegen.space.inline.md}"
            }
          }
        },
        "row": {
          "block-gap": {
            "$type": "spacing",
            "$value": "{nijmegen.space.block.md}"
          }
        },
        "meta-description": {
          "padding": {
            "inline": {
              "$type": "spacing",
              "$value": "{nijmegen.space.block.xs}"
            }
          }
        }
      },
      "hover": {
        "title": {
          "color": {
            "$type": "color",
            "$value": "{nijmegen.color.groen.500}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "{nijmegen.hover.text-decoration}"
          }
        },
        "link": {
          "color": {
            "$type": "color",
            "$value": "{nijmegen.color.groen.500}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "{nijmegen.hover.text-decoration}"
          }
        }
      },
      "focus-visible": {
        "border-color": {
          "$type": "color",
          "$value": "{nijmegen.color.groen.800}"
        }
      }
    }
  }
}