{
  "utrecht": {
    "breadcrumb": {
      "line-height": {
        "$type": "lineHeights",
        "$value": "{utrecht.document.line-height}"
      },
      "font-size": {
        "$type": "fontSizes",
        "$value": "{utrecht.document.font-size}"
      },
      "link": {
        "icon": {
          "size": {
            "$type": "sizing",
            "$value": "{nijmegen.icon.functional.size}"
          }
        },
        "padding-inline-end": {
          "$type": "spacing",
          "$value": "0px"
        },
        "padding-inline-start": {
          "$type": "spacing",
          "$value": "0px"
        },
        "color": {
          "$type": "color",
          "$value": "{nijmegen.interaction.color}"
        },
        "active": {
          "color": {
            "$type": "color",
            "$value": "{nijmegen.interaction.active.color}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "none"
          }
        },
        "focus": {
          "background-color": {
            "$type": "color",
            "$value": "{nijmegen.focus.background-color}"
          },
          "color": {
            "$type": "color",
            "$value": "{nijmegen.focus.color}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "none"
          }
        },
        "hover": {
          "color": {
            "$type": "color",
            "$value": "{nijmegen.interaction.hover.color}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "none"
          }
        },
        "current": {
          "color": {
            "$type": "color",
            "$value": "{nijmegen.color.grijs.500}"
          }
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "underline"
        }
      },
      "divider": {
        "size": {
          "$type": "sizing",
          "$value": "{nijmegen.size.150}"
        },
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.grijs.400}"
        }
      },
      "font-family": {
        "$type": "fontFamilies",
        "$value": "{utrecht.document.font-family}"
      },
      "font-weight": {
        "$type": "fontWeights",
        "$value": "{utrecht.document.font-weight}"
      }
    }
  }
}