{
  "nijmegen": {
    "card-event": {
      "content": {
        "padding": {
          "inline": {
            "$type": "spacing",
            "$value": "{nijmegen.space.inline.xl}"
          },
          "block-start": {
            "$type": "spacing",
            "$value": "{nijmegen.space.block.xl}"
          },
          "block-end": {
            "$type": "spacing",
            "$value": "{nijmegen.space.block.2xl}"
          }
        },
        "row": {
          "block-gap": {
            "$type": "spacing",
            "$value": "{nijmegen.space.block.md}"
          }
        }
      },
      "background-color": {
        "$type": "color",
        "$value": "{nijmegen.color.background-default}"
      },
      "title": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.zwart}"
        },
        "hover": {
          "color": {
            "$type": "color",
            "$value": "{nijmegen.color.groen.700}"
          }
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{nijmegen.typography.font-weight.semibold}"
        },
        "line-height": {
          "$type": "fontHeights",
          "$value": "{utrecht.heading-3.line-height}"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{nijmegen.typography.font-size.lg}"
        }
      },
      "date": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.grijs.500}"
        },
        "font-family": {
          "$type": "fontFamilies",
          "$value": "{nijmegen.typography.font-family.secondary}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{nijmegen.typography.font-weight.regular}"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{nijmegen.typography.font-size.md}"
        }
      },
      "place": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.grijs.700}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{nijmegen.typography.font-weight.regular}"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{nijmegen.typography.font-size.md}"
        }
      },
      "intro-text": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.grijs.500}"
        },
        "font-family": {
          "$type": "fontFamilies",
          "$value": "{nijmegen.typography.font-family.secondary}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{nijmegen.typography.font-weight.regular}"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{nijmegen.typography.font-size.md}"
        }
      },
      "icon": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.grijs.700}"
        }
      },
      "border": {
        "color": {
          "$type": "color",
          "$value": "{nijmegen.color.grijs.200}"
        }
      },
      "border-radius": {
        "$type": "borderRadius",
        "$value": "{nijmegen.border-radius.xs}"
      },
      "font-family": {
        "$type": "fontFamilies",
        "$value": "{nijmegen.typography.font-family.secondary}"
      }
    },
    "card-topic": {
      "icon": {
        "size": {
          "$type": "sizing",
          "$value": "{nijmegen.size.icon.md}"
        }
      }
    }
  }
}